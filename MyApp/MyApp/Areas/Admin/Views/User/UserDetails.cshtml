@model MyApp.Areas.Admin.Models.UserDetailsPageViewModel

@{
    ViewBag.Title = "GetUserDetails";
}

<h2>Your Details</h2>

<div class="row">
    <div class="col-md-8 col-md-offset-1">
        <table class="table table-striped">
            <tbody>
                <tr>
                    <th>LoginId</th>
                    <td>@Html.DisplayFor(model => model.LoginId)</td>
                </tr>
                <tr>
                    <th>Name</th>
                    <td>@Html.DisplayFor(model => model.Name)</td>
                </tr>
                <tr>
                    <th>MailAddress</th>
                    <td>@Html.DisplayFor(model => model.MailAddress)</td>
                </tr>
            </tbody>
        </table>
    </div>
</div>

